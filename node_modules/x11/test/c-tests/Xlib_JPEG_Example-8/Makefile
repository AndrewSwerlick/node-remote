CC=cc -g
CINCLUDE=-I/usr/X11R6/include -I/usr/local/include
CFLAGS=-Wall -g
LFLAGS= -L/usr/local/lib /home/laplace/libX11-1.4.0/src/.libs/libX11.so.6.3.0 -ljpeg -lm
#LFLAGS= -L/usr/local/lib /home/laplace/libX11-1.4.0/src/.libs/*.o /home/laplace/libX11-1.4.0/src/xcms/.libs/*.o /home/laplace/libX11-1.4.0/src/xlibi18n/.libs/*.o /home/laplace/libX11-1.4.0/src/xkb/.libs/*.o -ljpeg -lm

all: Xlib_JPEG_Example
	
Xlib_JPEG_Example: Xlib_JPEG_Example.c
	$(CC) $(CFLAGS) Xlib_JPEG_Example.c $(CINCLUDE) $(LFLAGS) -o Xlib_JPEG_Example

clean:
